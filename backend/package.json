    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "myapp": "file:..",
    "pg": "^8.11.3",
    "typescript": "^5.8.3",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
